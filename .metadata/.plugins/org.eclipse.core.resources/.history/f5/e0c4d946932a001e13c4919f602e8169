package ex02.nonDI;

public class NewRecordView {

	private NewRecord record;

	
	
	
	
	public void print() {

		System.out.println("KOR:" + record.getKor());
		System.out.println(record.total() + " / " + record.avg());
		System.out.println("+++++++++++++++++++++++++++++");
		record.disp();
		System.out.println(record); // toString() 함수 호출

	}

}
