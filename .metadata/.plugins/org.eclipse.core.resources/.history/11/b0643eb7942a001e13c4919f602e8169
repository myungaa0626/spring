package ex03.nonDI;

public class IRecordImpl implements IRecord { // DTO
	
	
	private int kor, eng, math, com;
	
	
	public int getKor() {
		return kor;
	}

	public void setKor(int kor) {
		this.kor = kor;
	}

	public int getEng() {
		return eng;
	}

	public void setEng(int eng) {
		this.eng = eng;
	}

	public int getMath() {
		return math;
	}

	public void setMath(int math) {
		this.math = math;
	}

	public int getCom() {
		return com;
	}

	public void setCom(int com) {
		this.com = com;
	}

	
	public IRecordImpl() {
		this(0,0,0);
		
	}
	
	
	
	@Override
	public int total() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public float avg() {
		// TODO Auto-generated method stub
		return 0;
	}

}
