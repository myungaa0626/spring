package ex05.di;

public class MemberDTO { // model
	
	private String name;
	private int age;
	private String message;
	

	
	
	
	public MemberDTO() {
		System.out.println("MemberDTO default constructor");
	}



	// constructor
	public MemberDTO(String name, int age, String message) {
		super();
		this.name = name;
		this.age = age;
		this.message = message;
	}
	
	

}
